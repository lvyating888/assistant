@import "../../app.wxss";
.search{
  position: fixed;
  left:0;
  top:0;
  background-color:#3571c9;
  width: 100%;
  height:128rpx;
  z-index:11;
}
.searchleft_li{
   padding:0 30rpx 0 24rpx;
   color:#fff;
   text-align: center;
   box-sizing: border-box;
   flex:1,
}
.searchtext_lis{
  font-size:32rpx;
  line-height:80rpx;
  white-space: nowrap;
}
.searchinputout{
  border-radius:12rpx;
  background-color: #fff;
  height:80rpx;
}
.searchinput{
  border:0;
  background:none;
  font-size:36rpx;
  color:#aaa;
}
.icon-qiandao,.icon-fenxiang{
  font-size:54rpx;
}
.icon-sousuo1{
    font-size:54rpx;  
    padding:0 16rpx;
    color:#aaa;
}
.search_inner{
  padding:0 0 0 30rpx;
}