@import "../../app.wxss";
.popadsout{
  position:fixed;
  width:100%;
  height:100%;
  top:0;
  left:0;
  z-index:111;
  overflow: hidden;
}
.popadsoutbg{
  position:absolute;
  width:100%;
  height:100%;
  top:0;
  left:0;
  background-color:rgba(0,0,0,0.4);
}
.register_pop{
  position: absolute;
  bottom:0;
  left: 0;
  z-index:1;
  height:670rpx;
  width:100%;
  background-color: #fff;
}
.register_pop_scroll{
  height:470rpx;
  width:100%;
}
.popadsimgs{
  width:536rpx;
  height:800rpx;
  border-radius:30rpx;
}
.close_btn{
  font-size:66rpx;
  margin-top:28rpx;
  text-align: center;
  color:#fff;
}
.searchout{
  position: absolute;
  left:0;
  top:0;
  background-color:#fff;
  z-index:11;
  width: 100%;
}
.search{
  height:114rpx;
}
.searchleft_li{
   padding:0 30rpx 0 24rpx;
   color:#333;
   text-align: center;
}
.searchtext_lis{
  font-size:32rpx;
  line-height:80rpx;
}
.searchinputout{
  border-radius:32rpx;
  background-color: #f7f7f7;
  height:80rpx;
}
.searchinput{
  border:0;
  background:none;
  font-size:32rpx;
  color:#aaa;
}
.icon-qiandao,.icon-fenxiang{
  font-size:54rpx;
}
.icon-sousuo1{
    font-size:44rpx;  
    padding:0 16rpx;
    color:#666666;
}
.search_inner{
  padding:0 0 0 30rpx;
}
.search_lis{
  border-bottom:2rpx solid #e6e6e6;
}
.search_complis{
    min-height:86rpx;
    padding-top:20rpx;
    padding-bottom:20rpx; 
    box-sizing:border-box;
}
.search_complis_text{
   font-size:28rpx;
   line-height:44rpx;
}
.search_complis_text_blue{
   color:#16a5af;
}
.search_complis_text_black{
   color:#000000;
}
.searchView{
   padding-top:200rpx;
}