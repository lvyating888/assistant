/* pages/houseDetails/houseDetails.wxss */
@import "/utils/common.wxss";
.swiper-container{
  position:relative;
}
.swiper-container,.swiper-box,.banner-item{
  width: 100%;
  height: 360rpx;
  margin:0;
  padding:0;
}
.slide-image{
  width: 100%;
  height: 100%;
}
.swiper_look{
  position:absolute;
  z-index:11;
  bottom:28rpx;
  left:28rpx;
}
.swiper_look_text{
  color:#fff;
  font-size:28rpx;
  padding-left:20rpx;
  padding-right:20rpx;
  padding-top:10rpx;
  padding-bottom:10rpx;
}
.swiper_look_bg{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,0.3);
  border-radius:22rpx;
  z-index:-1;
}
.forward,.forpk{
  width:66rpx;
  height:66rpx; 
  position: absolute;
  z-index: 11;
}
.forward_bg{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,0.3);
  z-index:-1;
  border-radius:50%;
}
.forward_bg_text{
  font-size:36rpx;
  color:#fff;
  text-align: center;
  line-height: 66rpx;
}
.forpk{
  right:28rpx;
  top:28rpx;
}
.forward{
  right:28rpx;
  top:122rpx;
}
.show_message{
  width: 100%;
  padding-top:20rpx;
  padding-bottom:20rpx;
}
.show_message_left{
  width:60%;
  padding-right: 5%;
}
.show_message_right{
  width:35%;
}
.show_message_title{
  font-size:36rpx;
  color:#000;
  padding-right:20rpx;
}
.show_message_hui{
   font-size:30rpx;
   color:#999999;
}
.show_message_black{
   font-size:30rpx;
   color:#333333;
}
.show_message_red{
   font-size:30rpx;
   color:red;
}
.show_smleft{
  width:30%;
  padding-right:5%;
}
.show_smright{
  width:65%;
}
.show_smlis{
  padding-top:12rpx;
  padding-bottom:12rpx;
}
.show_ewcode{
  width:200rpx;
  height:200rpx;
}
.big_btn{
  width: 100%;
  margin-top:25rpx;
  margin-bottom:25rpx;
  line-height:82rpx;
  text-align: center;
  background-color: #f7f7f7;
  border-radius:16rpx;
  font-size: 32rpx;
  color: #5c6584;
}
.navlis_two{
  min-width:20%;
  margin-bottom: 10rpx;
}
.navlis_two_img{
  font-size:46rpx;
  color:#67a7e9;
  padding:8rpx 0;
  text-align: center;
}
.navlis_two_text{
  line-height:34rpx;
  font-size:26rpx;
  text-align: center;
}
.housetabtd{
  /* width:25%; */
  padding-top:10rpx;
  padding-bottom:10rpx;
  text-align: center;
  border:1rpx solid #e4e4e4;
  font-size:30rpx;
  box-sizing: border-box;
}
.w25{
  width:25%;
  padding-left:2%;
  padding-right:2%;
}
.w20{
  width:20%;
  padding-left:2%;
  padding-right:2%;
  background-color: #f7f7f7;
}
.w60{
  width:60%;
  padding-left:2%;
  padding-right:2%;
  background-color: #f7f7f7;
}
.housetable{
  padding-top:20rpx;
  padding-bottom:20rpx;
}
.saleMessagelis{
  width:50%;
  box-sizing:border-box;
  padding-right:2%;
  padding-bottom:10rpx;
}
.saleMessageul .saleMessagelis:nth-child(even){
  padding-right:0;
}
.saleMessagelis .show_smleft{
  min-width: 30%;
  width: auto;
  white-space:nowrap;
}
.saletitle{
  font-size:36rpx;
  color:#000;
  padding-top:20rpx;
  padding-bottom:20rpx;
}
