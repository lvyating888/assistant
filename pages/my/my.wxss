.my_out{
  position: absolute;
  background-color: #f7f7f7;
}
.my_top{
  min-height:246rpx;
  background-color: #2b8ceb;
  padding-left:30rpx;
}
.user_img{
  width:96rpx;
  height:96rpx;
  border-radius:50%;
  margin-right:24rpx;
}
.user_name{
  font-size: 32rpx;
  color:#fff;
  background: none;
  border-color:#6eb5f7;
  text-align:left;
  padding-left:0;
  padding-right:0;
}
button.user_name::after{
  border: none;
}
.share_btn{
  padding-left:22rpx;
  padding-right:22rpx;
  height:48rpx;
  background-color: #6eb5f7;
  padding-top:8rpx;
  padding-bottom:8rpx;
  margin-left:14rpx;
}
.share_btn_one{
  border-radius:34rpx;
}
.share_btn_two{
  border-top-left-radius: 34rpx;
  border-bottom-left-radius: 34rpx;
}
.share_btn_icon{
  font-size:34rpx;
  margin-right:12rpx;
  color:#fff;
}
.share_btn_text{
  color:#fff;
  font-size:28rpx;
}
.tabul{
  background-color: #fff;
  border-radius:24rpx;
  padding-top:18rpx;
  padding-bottom:22rpx;
  margin-bottom:20rpx;
}
.tablis{
  padding-left:36rpx;
  padding-right:36rpx;
}
.tablis_icon{
  font-size:60rpx;
  padding-top:10rpx;
  padding-bottom:10rpx;
  text-align: center;
}
.tablis_text{
  line-height:40rpx;
  font-size:26rpx;
  color:#000000;
  text-align: center;
}
.my_bottom{
  margin-top:-34rpx;
}
.tablis_yellow{
  color: #f0a44c;
}
.tablis_blue{
  color: #2b8ceb;
}
.tablis_title{
  line-height:58rpx;
  font-size:30rpx;
  color:#000000;
}